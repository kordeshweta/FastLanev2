{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],

  
  "dataGroups": [{
    "name": "APIs",
    "urls": [
      
      "/api//inauguration",
      "/api/isAdmin",
      "/api/**"
      
      
     
    ],
    "cacheConfig": {
      "strategy": "performance",
      "maxSize": 300,
      "maxAge": "3d",
      "timeout": "100s"
    }
  }
]
}
